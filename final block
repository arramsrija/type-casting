package thread;

public class FinallyBlockDemo {

	public static void main(String[] args) {
		float a=85.36f ,b=5, c=0;
        try
        {
            c = a / b;
        }
        catch(ArithmeticException Ex)
        {
            System.out.print("\n\tError : " + Ex.getMessage());
        }
        finally
        {
            System.out.print("\n\tThe result is : " + c);
        }


	}

}
